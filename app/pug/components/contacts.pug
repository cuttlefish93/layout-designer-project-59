section.contacts
  h2.contacts-title Контакты
  include search-panel.pug
  ul.contacts-list.list-unstyled
    each users, char in contacts
      li.contacts-list-item.pt-5
        p.contacts-list-char= char
        each user in users
          a.contact.px-3.py-4.text-decoration-none.d-flex.align-items-center.column-gap-4.shadow-sm(href='#')
            img.contact-avatar(src=user.avatar)
            div.contact-info
              p.contact-name= user.name
              p.contact-status= user.status

